                <Canvas.LayoutTransform>
                    <TransformGroup>
                        <ScaleTransform x:Name="myScaleTransform" />
                    </TransformGroup>
                </Canvas.LayoutTransform>
            </Canvas>
        </Viewbox>
        <TextBox  x:Name="fsr1_box" Height="22" Margin="277.044,112,37.456,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="4" IsReadOnly="True"/>
        <TextBox  x:Name="fsr2_box" Height="22" Margin="277.044,169,37.456,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="4" IsReadOnly="True"/>
        <TextBox  x:Name="fsr3_box" Height="22" Margin="148.044,190,166.456,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="4" IsReadOnly="True"/>
        <TextBox  x:Name="fsr4_box" Height="22" Margin="148.044,250,166.456,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="4" IsReadOnly="True"/>
        <TextBox  x:Name="fsr5_box" Height="22" Margin="215.044,360,99.456,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="4" IsReadOnly="True"/>

        <TextBox  x:Name="fsr6_box" Height="23" Margin="2.544,112,169.279,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="5" Grid.ColumnSpan="2" IsReadOnly="True"/>
        <TextBox  x:Name="fsr7_box" Height="23" Margin="1.544,169,170.279,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="5" Grid.ColumnSpan="2"  IsReadOnly="True"/>
        <TextBox  x:Name="fsr8_box" Height="23" Margin="88.544,190,37.279,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="6" IsReadOnly="True"/>
        <TextBox  x:Name="fsr9_box" Height="23" Margin="88.544,250,37.279,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="6" IsReadOnly="True"/>
        <TextBox  x:Name="fsr10_box" Height="23" Margin="23.544,360,102.279,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="6" RenderTransformOrigin="0.512,1.815" IsReadOnly="True"/>

       
        <Label x:Name="label_Copy1" Content="Height" HorizontalAlignment="Left" Margin="307.205,497,0,-107" VerticalAlignment="Top" Width="62" Height="34" Grid.Column="4" Grid.ColumnSpan="2" Visibility="Hidden"/>
        <TextBox x:Name="time_box" HorizontalAlignment="Left" Height="23" Margin="303.205,631,0,-230" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" Text="0" Grid.Column="4" Grid.ColumnSpan="2" Visibility="Hidden"/>
        <Label x:Name="label_Copy2" Content="Port" HorizontalAlignment="Left" Margin="146.205,429,0,-39" VerticalAlignment="Top" Width="38" Height="34" Grid.Column="4"/>
        <Label x:Name="label_Copy3" Content="통신속도" HorizontalAlignment="Left" Margin="296.044,429,0,-39" VerticalAlignment="Top" Width="59" Height="34" Grid.Column="4"/>
    </Grid>
