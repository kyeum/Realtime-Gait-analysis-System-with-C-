            <ColumnDefinition Width="55*"/>
            <ColumnDefinition Width="135*"/>
            <ColumnDefinition Width="360*"/>
            <ColumnDefinition Width="46*"/>
            <ColumnDefinition Width="171*"/>
        </Grid.ColumnDefinitions>
        <Image Name="ca" Width="512" Height="424" Margin="-4,90,239,0" Grid.ColumnSpan="5" RenderTransformOrigin="0.5,0.5" >
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="1" ScaleX="-1"/>
                    <SkewTransform AngleY="0" AngleX="0"/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Canvas Name="can" Width="512" Height="424" Margin="-4,90,239,0" Grid.ColumnSpan="5" RenderTransformOrigin="0.5,0.5">
            <Canvas.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="1" ScaleX="-1"/>
                    <SkewTransform AngleY="0" AngleX="0"/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Canvas.RenderTransform>
        </Canvas>
        <Canvas Name="linedr" Width="512" Height="424" Margin="-4,90,239,0" Grid.ColumnSpan="5" RenderTransformOrigin="0.5,0.5">
            <Canvas.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="1" ScaleX="-1"/>
                    <SkewTransform AngleY="0" AngleX="0"/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Canvas.RenderTransform>
        </Canvas>
        <TextBlock x:Name="xfactortext" Margin="0,0,53,405" Width="200" Foreground="Aqua" Grid.ColumnSpan="3" ></TextBlock>
        <TextBlock x:Name="Momentumtext" Margin="0,24,53,381" Width="200" Foreground="Aqua" Grid.ColumnSpan="3" ></TextBlock>
